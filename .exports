export JAVA_HOME=/usr/lib/jvm/java-18-openjdk
#export JAVA_HOME=/home/pgmillon/Applications/jdk1.8.0_101
export JAVA_PATH=$JAVA_HOME/bin/java
export ANT_HOME=/home/pgmillon/Applications/apache-ant-1.7.1
